@model IEnumerable<ENETCareMVCApp.Models.District>

@{
    ViewBag.Title = "DistrictListForMonthlyCost";
}

<h2>District List For Monthly Cost</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DistrictName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DistrictName)
        </td>
        <td>
            @Html.ActionLink("Monthly Report", "MonthlyCostsForDistrict", new { id=item.DistrictID })
        </td>
    </tr>
}

</table>
